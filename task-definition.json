{
  "family": "my-ecs-app",
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "requiresCompatibilities": ["FARGATE"],
  "executionRoleArn": "arn:aws:iam::593793047834:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "IMAGE_PLACEHOLDER",
      "portMappings": [{ "containerPort": 3000, "protocol": "tcp" }],
      "essential": true
    }
  ]
}
