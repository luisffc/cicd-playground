{
  "family": "my-ecs-app",
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "requiresCompatibilities": ["FARGATE"],
  "executionRoleArn": "arn:aws:iam::593793047834:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "environment": [],
      "essential": true,
      "image": "IMAGE_PLACEHOLDER",
      "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
              "awslogs-group": "/ecs/getting-started",
              "awslogs-region": "us-east-1",
              "awslogs-stream-prefix": "ecs"
          }
      },
      "mountPoints": [],
      "name": "app",
      "portMappings": [
          {
              "containerPort": 80,
              "hostPort": 80,
              "protocol": "tcp"
          }
      ],
      "systemControls": [],
      "volumesFrom": []
  }
  ]
}
